#include<iostream>
#include<algorithm>
#include<cmath>
#include<string>

using namespace std;


bool desc(int a, int b) {
	return a > b;
}

int main()
{
	int arr[11] = { 0, };
	int n;
	cin >> n;
	string str;
	str = to_string(n);
	for (int i = 0; i < str.length(); i++) {
		arr[i] = str[i] - '0';
	}
	sort(arr, arr + str.length(), desc);
	for (int i = 0; i < str.length(); i++) {
		cout << arr[i];
	}

}
