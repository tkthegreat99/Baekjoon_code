#include<iostream>
#include<algorithm>
#include<cmath>
#include<string>
#include<vector>



using namespace std;
vector<pair<int, int>> v;





bool desc(int a, int b) {
	return a > b;
}


int main()
{
	int n;
	cin >> n;
	int x, y;
	for (int i = 0; i < n; i++) {
		cin >> x >> y;
		v.push_back({ y, x });
	}
	sort(v.begin(), v.end());
	for (int i = 0; i < n; i++) {
		cout << v[i].second << ' ' << v[i].first << '\n';
	}
	


}
